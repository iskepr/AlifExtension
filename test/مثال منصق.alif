تتبع_سقوط_الكرة(الارتفاع_الاولي = ارتفاع, الكتلة = كتلة)

# الجزور التربيعية

دالة معادلة_تربيعية(أ, ب, ج):
	المميز = ب ^ 2 - 4 * أ * ج
	اذا المميز  > 0:
		الجذر1 = (-ب + \^ المميز) \ (2*أ)
		الجذر2 = (-ب - \^ المميز) \ (2*أ)
		ارجع الجذر1, الجذر2
	اواذا المميز == 0:
		الجذر = -ب \ (2 * أ)
		ارجع الجذر
	والا:
		ارجع "لا يوجد جذر حقيقي -الجذر تخيلي-"

